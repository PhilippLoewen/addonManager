{
  "manifest_version": 2,
  "name": "management",
  "description": "manage all installed addons",
  "version": "0.0.1",
  "applications": {
    "gecko": {
      "id": "addonManager@web-ext-labs",
      "strict_min_version": "55.0"
    }
  },
  "icons": {
    "64": "icons/icon.png"
  },
  "background": {
    "scripts": [
      "background_script.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "64": "icons/icon.png"
    },
    "default_title": "management"
  },
  "permissions": ["management"]
}